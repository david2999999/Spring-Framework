<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	
	<bean id="myorg" class="com.spring.pnamespace.domain.Organization" 
		p:companyName="BMW" p:yearOfIncorporation="1929" p:address-ref="address"/>
		
	<bean id="address" class="com.spring.pnamespace.domain.Address" 
		p:street="Red Berry Lane" p:city="Los Angelos" p:zipCode="121233"/>
		
</beans>
