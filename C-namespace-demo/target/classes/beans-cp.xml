<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	
	<bean id="myorg" class="com.spring.cnamespace.domain.Organizaton" 
		c:companyName="BMW" c:yearOfIncorporation="1929" c:address-ref="address" />
		
	<bean id="address" class="com.spring.cnamespace.domain.Address"
		c:city="Los Angeles" c:street="Red Berry Lane" c:zipCode="112122" />
		
</beans>
